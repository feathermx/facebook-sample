<% url = el['picture']['data']['url'] %>
<% id = el['id'] %>
<div class="col-xs-6 col-sm-4 col-md-3 col-lg-2">
  <div class="panel panel-default friend">
    <div class="panel-heading friend-name">
      <%= el['name'] %>
    </div>
    <div class="panel-body">
      <%= image_tag url, class: 'img-responsive img-rounded center-block' %>
    </div>
  </div>
</div>
